# APT Publishing Environment Variables
# Copy this file to .env and fill in your API keys

# Gumroad API - Get from: https://gumroad.com/settings/applications
# Follow: https://gumroad.com/help/article/280-create-application-api
GUMROAD_ACCESS_TOKEN=your_gumroad_access_token_here

# Itch.io API
ITCH_API_KEY=your_itch_api_key_here
ITCH_USER=your_itch_username
ITCH_GAME=your_game_slug

# Paddle API
PADDLE_API_KEY=your_paddle_api_key_here
PADDLE_PRODUCT_ID=your_product_id

# GitHub API
GITHUB_TOKEN=your_github_personal_access_token
GITHUB_REPO=yavru421/chronogrid-video-processor